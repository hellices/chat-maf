PUBLIC_API_BASE_URL=http://localhost:8000

# OpenTelemetry Configuration
# Set to 'true' to enable OpenTelemetry tracing
PUBLIC_OTLP_ENABLED=true

# OTLP endpoint for sending traces (e.g., Aspire Dashboard)
# Use HTTP OTLP endpoint (port 18890 for local, 4318 for AKS)
# Local development: http://localhost:18890/v1/traces
# AKS deployment: http://<EXTERNAL-IP>:4318/v1/traces
PUBLIC_OTLP_ENDPOINT=http://localhost:18890/v1/traces
