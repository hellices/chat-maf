################### Backend Configuration #######################

# OpenTelemetry Configuration
# Set to "true" to enable telemetry, "false" to disable
OTLP_ENABLED=true

# OTLP gRPC endpoint for sending traces, metrics, and logs
# For local Aspire Dashboard: http://localhost:4317
# For AKS deployment: http://<EXTERNAL-IP>:4317
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317

# Enable sensitive data logging (for development only)
ENABLE_SENSITIVE_DATA=false

# Azure OpenAI Configuration
AZURE_OPENAI_API_KEY=your-api-key-here
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_RESPONSES_DEPLOYMENT_NAME=gpt-4.1
AZURE_OPENAI_CHAT_DEPLOYMENT_NAME=gpt-4.1
AZURE_OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# Azure AI Search Configuration
AZURE_SEARCH_ENDPOINT=https://your-search-service.search.windows.net
AZURE_SEARCH_API_KEY=your-search-api-key-here
AZURE_SEARCH_INDEX_NAME=your-index-name

# Quality Evaluation Settings
QUALITY_THRESHOLD=80

# Web Scraper Configuration
# Request timeout in seconds for web scraping
WEB_SCRAPER_TIMEOUT=30
# NL2SQL Workflow Configuration Example
# Copy this to .env and adjust values as needed

# NL2SQL Agent Models
# Each agent can use a different model for optimization
NL2SQL_ENTITY_EXTRACTOR_MODEL=gpt-4o
NL2SQL_SCHEMA_MAPPER_MODEL=gpt-4o  
NL2SQL_SQL_GENERATOR_MODEL=gpt-4o
NL2SQL_ERROR_CORRECTOR_MODEL=gpt-4o

# Confidence Score Threshold
# Minimum confidence score to accept (0-100)
NL2SQL_MIN_CONFIDENCE_SCORE=70.0
